#!/home/sebastien/.conda/envs/python/bin/python

# Run the clash-testbench python tool
from clash_testbench import Clashi
from glob import glob
from json import load

def main():
    print("This is the clash-testbench tool, V0.1")
    print("SÃ©bastien Deriaz - 22.11.2022")


    for testbench_template in glob('**/*.json'):
        with open(testbench_template, encoding='utf-8') as f:
            template = load(f)
            print(template)
            



    clashi = Clashi()
    clashi.load(b'MR_FSK/PHR.hs')













if __name__ == '__main__':
    main()